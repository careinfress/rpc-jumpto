<idea-plugin>
    <id>com.fkw.plugin.jnetkit.rpc-jumpto</id>
    <name>Doodle Jump</name>
    <vendor email="careinfress@gmail.com" url="https://github.com/careinfress/rpc-jumpto">careinfress</vendor>

    <description><![CDATA[
    A Plugin Of Jumpto For RPC.<br>
    <p><b>一款方便RPC跳转的插件</b></p>
    ]]></description>


    <idea-version since-build="145"/>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
        <codeInsight.lineMarkerProvider language="JAVA" implementationClass="com.fkw.rpc.provider.FaiCliLineMakerProvider"/>
        <codeInsight.lineMarkerProvider language="JAVA" implementationClass="com.fkw.rpc.provider.JNetkitAnnotationLineMarkerProvider"/>
        <codeInsight.lineMarkerProvider language="JAVA" implementationClass="com.fkw.rpc.provider.NetkitAnnotationLineMarkerProvider"/>
        <!--<codeInsight.lineMarkerProvider language="JAVA" implementationClass="com.fkw.rpc.provider.FaiSvrLineMarkerProvider"/>-->
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>